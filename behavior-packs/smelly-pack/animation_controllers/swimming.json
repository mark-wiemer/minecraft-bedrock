{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.swimming": {
      "states": {
        "default": {
          "transitions": [
            {
              "scroll": "query.is_swimming"
            }
          ],
          "on_exit": ["/tag @s add swimming"]
        },
        "scroll": {
          "transitions": [
            {
              "default": "!query.is_swimming"
            }
          ],
          "on_exit": ["/tag @s remove swimming"]
        }
      }
    }
  }
}
